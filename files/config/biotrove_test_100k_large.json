{
  "input": "files/input/biotrove_test_100k_large.parquet",
  "input_format": "parquet",
  "output": "files/output/biotrove_test_100k_large",
  "output_format": "webdataset",
  "url": "url",
  "label": "species",

  "concurrent_downloads": 1500,
  "timeout": 30,

  "enable_paarc": true,
  "C_init": 8,
  "C_min": 2,
  "C_max": 1500,
  "mu": 0.85,
  "startup_theta_50": 16.0,
  "startup_theta_95": 24.0,
  "startup_additive_increase": 30,
  "probe_bw_additive_increase": 10,
  "efficiency_threshold": 0.5,
  "efficiency_window": 5,
  "beta": 0.5,
  "theta_50": 12.0,
  "theta_95": 20.0,
  "probe_rtt_period": 30.0,
  "rtprop_window": 35.0,
  "cooldown_floor": 3.0,
  "alpha_ema": 0.3,

  "max_retry_attempts": 3,
  "retry_backoff_sec": 2.0,
  "naming_mode": "url_based",
  "file_name_pattern": "{segment_parent}",
  "create_tar": true,
  "compress_tar": false,
  "create_overview": true
}
