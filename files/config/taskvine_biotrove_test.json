    "mu": 0.85,
    "startup_theta_50": 16.0,
    "startup_theta_95": 24.0,
    "efficiency_threshold": 0.5,
    "efficiency_window": 5,
    "beta": 0.5,
    "theta_50": 12.0,
    "theta_95": 20.0,
    "probe_rtt_period": 30.0,
    "rtprop_window": 35.0,
    "cooldown_floor": 3.0,
    "alpha_ema": 0.3,

    "max_retry_attempts": 3,
    "retry_backoff_sec": 2.0,
    "naming_mode": "url_based",
    "file_name_pattern": "{segment_parent}",
    "create_tar": true,
    "compress_tar": false,
    "create_overview": true,

    "task_cores": 4,
    "task_memory_mb": 28000,
    "task_disk_mb": 30000,
    "timeout_minutes": 60
