{
  "input": "files/input/TaiBIF-Test.parquet",
  "input_format": "parquet",
  "output": "files/output/TaiBIF-Test ",
  "output_format": "imagefolder",

  "url": "url",
  "label": "species",

  "concurrent_downloads": 5000,
  "timeout": 30,

  "enable_paarc": true,

  "C_init": 8,
  "C_min": 2,
  "C_max": 2000,
  "mu": 0.95,
  "startup_theta_50": 4.0,
  "startup_theta_95": 8.0,
  "beta": 0.7,
  "theta_50": 4,
  "theta_95": 8,
  "probe_rtt_period": 30.0,
  "rtprop_window": 35.0,
  "cooldown_floor": 2.0,
  "alpha_ema": 0.3,

  "max_retry_attempts": 3,
  "retry_backoff_sec": 2.0,

  "naming_mode": "sequential",
  "file_name_pattern": "{segment[-2]}",

  "create_tar": false,
  "create_overview": true
}
